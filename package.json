{
  "name": "baseline-buddy",
  "displayName": "Baseline Buddy",
  "description": "A VS Code extension to help with baseline data checks, quick fixes, and developer productivity.",
  "version": "0.0.1",
  "publisher": "shubhm8971",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:baseline-buddy.helloWorld",
    "onCommand:baseline-buddy.quickFix"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "baseline-buddy.helloWorld",
        "title": "Baseline Buddy: Hello World"
      },
      {
        "command": "baseline-buddy.quickFix",
        "title": "Baseline Buddy: Quick Fix"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0",
    "@types/mocha": "^10.0.0"
  }
}
